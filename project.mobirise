          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1 elevatem5\" group=\"Footers\" global=\"\" once=\"footers\" mbr-class=\"{'mbr-fullscreen': fullScreen, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked=\"\">\n        <header>Social</header>\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"0\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"color\" title=\"Social Color\" value=\"#ffffff\" name=\"social\" selected=\"\">\n        <header>Lists</header>\n        <input type=\"range\" inline=\"\" title=\"Amount\" name=\"listsAmount\" min=\"0\" max=\"4\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"List Title\" name=\"showListTitle\" checked=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"\" parallax=\"\">\n            <input type=\"color\" title=\"Color\" value=\"#7787a5\" selected=\"\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-4 card\">\n                <div class=\"content-wrapper\">\n                    <div class=\"title-wrapper\" mbr-if=\"showLogo || showTitle\">\n                        <div class=\"title-wrap\">\n                            <img src=\"@PROJECT_PATH@/assets/images/whatsapp-image-2023-01-27-at-13.24.38-3-removebg-preview.png\" alt=\"Mobirise\" mbr-if=\"showLogo\">\n                            <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .title-wrapper, .social-wrapper\">\n                                <b>Aguas del Municipio de Durango</b></h2>\n                        </div>\n                    </div>\n                    <p class=\"copyright mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCopy\" data-app-selector=\".copyright\">Aquatel: (618) 1500 600<br><br><br></p>\n                </div>\n                <div class=\"social-wrapper\">\n                    <div class=\"social-wrap\">\n                        <a href=\"https://www.facebook.com/AMDurango/?locale=es_LA\" target=\"_blank\"><span mbr-icon=\"\" class=\"mbr-iconfont socicon-facebook socicon\" mbr-if=\"socialCount > 0\"></span></a>\n                        <a href=\"https://twitter.com/AMD_Durango\" target=\"_blank\"><span mbr-icon=\"\" class=\"mbr-iconfont socicon-twitter socicon\" mbr-if=\"socialCount > 1\"></span></a>\n                        <a href=\"https://api.whatsapp.com/send?phone=526181002021&amp;text=Buen%20dia\" target=\"_blank\"><span mbr-icon=\"\" class=\"mbr-iconfont socicon-whatsapp socicon\" mbr-if=\"socialCount > 2\"></span></a>\n                        <a href=\"https://www.instagram.com/amd_durango/\" target=\"_blank\"><span mbr-icon=\"\" class=\"mbr-iconfont socicon-instagram socicon\" mbr-if=\"socialCount > 3\"></span></a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-6\" mbr-if=\"listsAmount > 0\">\n                <div class=\"list-wrapper\">\n                    <h4 class=\"mbr-card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showListTitle\" data-app-selector=\".mbr-card-title\"><b>\n                        Trámites y Servicios</b></h4>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline=\"\" mbr-article=\"\">\n                        <li class=\"item-wrap\"><a href=\"http://187.191.95.55/FacturacionRegistro/index.html\" class=\"text-white\" target=\"_blank\" style=\"font-size: 1rem;\">Facturación</a><br></li><li class=\"item-wrap\"><a href=\"http://187.191.95.55/ReciboMail/index.html\" class=\"text-white\" target=\"_blank\">Recibo en línea\n</a></li><li class=\"item-wrap\"><a href=\"servicios.html\" class=\"text-white\">Instalación de medidor\n</a></li><li class=\"item-wrap\"><a href=\"servicios.html\" class=\"text-white\">Contratación de servicios de agua</a></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-6\" mbr-if=\"listsAmount > 1\">\n                <div class=\"list-wrapper\">\n                    <h4 class=\"mbr-card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showListTitle\" data-app-selector=\".mbr-card-title\"><b>\n                        Servicios en Línea</b></h4>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline=\"\" mbr-article=\"\">\n                        <li class=\"item-wrap\"><a href=\"http://187.191.95.55/pagosamd/comercio.html\" class=\"text-white\" target=\"_blank\">Pagos</a>\n</li><li class=\"item-wrap\">Reportes\n</li><li class=\"item-wrap\"><a href=\"http://187.191.95.55/Duplicadosprueba/index.html\" class=\"text-white\" target=\"_blank\">Duplicados</a>\n</li><li class=\"item-wrap\"><a href=\"http://187.191.95.55/ReciboMail/index.html\" class=\"text-white\" target=\"_blank\">Estados de cuenta\n</a></li><li class=\"item-wrap\"><a href=\"http://187.191.95.55/transparencia/index.html\" class=\"text-white\" target=\"_blank\">Hipervínculos</a></li><li class=\"item-wrap\"><a href=\"http://187.191.95.55/nomina/\" class=\"text-white\" target=\"_blank\">Acceso personal AMD</a></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-6\" mbr-if=\"listsAmount > 2\">\n                <div class=\"list-wrapper\">\n                    <h4 class=\"mbr-card-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showListTitle\" data-app-selector=\".mbr-card-title\"><b>\n                        Información</b><b><br></b></h4>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline=\"\" mbr-article=\"\">\n                        <li class=\"item-wrap\"><a href=\"assets/files/Programa-AnualPROAGUA.pdf